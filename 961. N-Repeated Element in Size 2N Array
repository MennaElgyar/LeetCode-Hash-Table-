class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {

        int repeatedNTimes(vector<int>& nums)
 {
     int res;
      int _size= nums.size();
     sort(nums.begin(),nums.end());
     for(int i=0;i<_size-1;i++)
     {
         if(nums[i]==nums[i+1])
         {
             res=nums[i];
             break;
         }
     }
     return res;
 }
};
